# 服务器配置
PORT=11434

# 火山方舟引擎配置 - API Key 认证方式
VOLCENGINE_API_KEY=your_api_key_here

# 阿里云百炼配置 - API Key 认证方式
DASHSCOPE_API_KEY=your_api_key_here

# 腾讯云DeepSeek配置 - API Key 认证方式
TENCENTDS_API_KEY=your_tencent_deepseek_api_key_here

# DeepSeek官方配置 - API Key 认证方式
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Moonshot AI配置 - API Key 认证方式
MOONSHOT_API_KEY=your_moonshot_api_key_here

# OpenRouter AI模型聚合服务配置 - API Key 认证方式
OPENROUTER_API_KEY=your_openrouter_api_key_here

# 开发环境配置
NODE_ENV=development

# 日志级别配置 (debug, info, warn, error)
# debug: 输出所有级别日志
# info: 输出 info、warn 和 error 级别日志
# warn: 只输出 warn 和 error 级别日志
# error: 只输出 error 级别日志
LOG_LEVEL=info

# 聊天详细日志配置
# 启用聊天详细日志记录 (true/false)
CHAT_LOGS=false
# 聊天日志存储目录 (默认: logs/chat)
CHAT_LOGS_DIR=logs/chat
