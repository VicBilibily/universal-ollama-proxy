{
  // 统一配置文件 - 支持所有AI提供商的配置
  "providers": [
    {
      // 火山方舟引擎配置 - 字节跳动旗下AI服务
      "name": "volcengine",
      "displayName": "火山方舟引擎",
      "enabled": true, // 设置为false可临时停用该提供商
      "baseURL": "https://ark.cn-beijing.volces.com/api/v3",
      "apiKey": "${VOLCENGINE_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      // 阿里云百炼平台配置
      "name": "dashscope",
      "displayName": "阿里云百炼",
      "enabled": true,
      "baseURL": "https://dashscope.aliyuncs.com/compatible-mode/v1",
      "apiKey": "${DASHSCOPE_API_KEY}",
      "headers": {
        "Content-Type": "application/json",
        "X-DashScope-SSE": "enable"
      }
    },
    {
      // 深度求索AI配置
      "name": "deepseek",
      "displayName": "DeepSeek官方",
      "enabled": true,
      "baseURL": "https://api.deepseek.com/v1",
      "apiKey": "${DEEPSEEK_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      // 腾讯云DeepSeek配置
      "name": "tencentds",
      "displayName": "腾讯云DeepSeek",
      "enabled": true,
      "baseURL": "https://api.lkeap.cloud.tencent.com/v1",
      "apiKey": "${TENCENTDS_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      // Moonshot AI配置 - Kimi大模型
      "name": "moonshot",
      "displayName": "Moonshot AI",
      "enabled": true,
      "baseURL": "https://api.moonshot.cn/v1",
      "apiKey": "${MOONSHOT_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      // OpenRouter AI模型聚合服务配置
      "name": "openrouter",
      "displayName": "OpenRouter",
      "enabled": true,
      "baseURL": "https://openrouter.ai/api/v1",
      "apiKey": "${OPENROUTER_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      // 魔搭社区配置 - 阿里云开源模型社区
      "name": "modelscope",
      "displayName": "魔搭社区",
      "enabled": true,
      "baseURL": "https://api-inference.modelscope.cn/v1",
      "apiKey": "${MODELSCOPE_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      // BigModel AI配置 - GLM系列模型
      "name": "bigmodel",
      "displayName": "智谱AI开放平台",
      "enabled": true,
      "baseURL": "https://open.bigmodel.cn/api/paas/v4",
      "apiKey": "${BIGMODEL_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
        // 以下为使用订阅套餐的请求头，可根据需要启用
        // "HTTP-Referer": "https://kilocode.ai",
        // "X-Title": "Kilo Code",
        // "X-KiloCode-Version": "4.91.2",
        // "User-Agent": "Kilo-Code/4.91.2",
        // "Authorization": "Bearer YOUR_BIGMODEL_API_KEY"
      }
    },
    {
      // iFlow 心流平台配置 - OpenAI兼容接口
      "name": "iflow",
      "displayName": "iFlow心流平台",
      "enabled": true,
      "baseURL": "https://apis.iflow.cn/v1",
      "apiKey": "${IFLOW_API_KEY}",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ],
  "meta": {
    "version": "1.0.0",
    "lastUpdated": "2025-06-03",
    "description": "AI服务供应商配置文件"
  }
}
